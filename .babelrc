{
  "env": {
    "development": {
      "comments": true,
      "compact": false,
      "minified": false
    },
    "production": {
      "comments": false,
      "compact": true,
      "minified": true
    }
  },
  "plugins": [
    "transform-class-properties"
  ],
  "presets": [
    "react",
    "es2015",
    "stage-2",
    [
      "env",
      {
        "targets": {
          "browsers": ["last 2 versions"]
        }
      }
    ]
  ],
  "ignore": [
    ".scss"
  ]
}
